{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"$lib": ["src/lib"],
			"$lib/*": ["src/lib/*"],
			"$components/*": ["src/_components/*"],
			"$elements/*": ["src/_elements/*"],
			"$utils/*": ["src/_utils/*"],
			"$dataSources/*": ["src/_dataSources/*"],
			"$stores/*": ["src/_stores/*"],
			"$machines/*": ["src/_machine/*"]
		}
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"]
}
